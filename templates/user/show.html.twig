{% extends 'base.html.twig' %}
{% block title %}UserController
{% endblock %}
{% block body %}
	<h4>Hello !</h4>


	<ul>
		{% if is_granted('ROLE_FORMATEUR') %}
			{% for user in users %}
				<li>{{ user.id }}
					{{ user.prenom }}
					{{ user.nom }}
				</li>
			{% endfor %}
		{% endif %}

	</ul>
{% endblock %}
